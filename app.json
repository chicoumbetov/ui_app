{
  "expo": {
    "name": "omedom",
    "slug": "omedom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icones_omedom/logo_menu_principal.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Icones_omedom/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "backgroundColor": "#ffffff",
      "usesIcloudStorage": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icones_omedom/logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon-32x32.png",
      "background_color": "#ffffff",
      "splash": {
        "image": "./assets/favicon-32x32.png",
        "background_color": "#ffffff",
        "resizeMode": "cover"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "ts", "tsx", "svg"]
    }
  }
}
